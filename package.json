{
  "name": "note",
  "version": "0.0.0",
  "description": "Personal Note App",
  "author": "kigh-ota",
  "keywords": [
    "note",
    "electron"
  ],
  "main": "./app/dist/main.js",
  "scripts": {
    "webpack:dev": "webpack --colors --progress",
    "webpack:prod": "NODE_ENV=production webpack -p --colors --progress",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "flow": "flow",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty . --ext .js --ext .jsx",
    "test:unit": "BABEL_ENV=test mocha --compilers js:babel-core/register ./app/tests/unit/**/*.spec.js",
    "test:integration": "BABEL_ENV=test mocha --compilers js:babel-core/register ./app/tests/integration/**/*.spec.js",
    "test": "npm run test:unit && npm run test:integration",
    "ci": "npm run flow && npm run lint && npm run test",
    "start:prod": "electron .",
    "start:dev": "DEVELOPMENT=1 electron .",
    "dev": "npm run webpack:dev && npm run start:dev",
    "prod": "npm run webpack:prod && npm run start:prod"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-sqlite3": "^0.2.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.10",
    "electron-rebuild": "^1.5.11",
    "eslint": "^3.19.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.47.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.1",
    "mocha": "^3.4.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "reset-css": "^2.2.0",
    "spectron": "^3.6.4",
    "sqlite": "^2.8.0",
    "string.prototype.padstart": "^3.0.0",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "sqlite3": "^3.1.8"
  }
}
