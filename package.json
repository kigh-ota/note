{
  "name": "note",
  "version": "0.0.0",
  "description": "Personal Note App",
  "author": "kigh-ota",
  "keywords": [
    "note",
    "electron"
  ],
  "main": "./app/dist/main.js",
  "scripts": {
    "webpack:dev": "webpack --progress --colors",
    "webpack:prod": "NODE_ENV=production webpack -p --progress --colors",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "flow": "flow",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty . --ext .js --ext .jsx",
    "test:unit": "BABEL_ENV=test mocha --compilers js:babel-core/register ./app/tests/unit/**/*.spec.js",
    "test:integration": "BABEL_ENV=test mocha --compilers js:babel-core/register ./app/tests/integration/**/*.spec.js",
    "test": "npm run test:unit && npm run test:integration",
    "ci": "npm run flow && npm run lint && npm run test",
    "start": "electron .",
    "debug": "DEVELOPMENT=1 electron ."
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.10",
    "electron-rebuild": "^1.5.11",
    "eslint": "^3.19.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "immutable": "^3.8.1",
    "mocha": "^3.4.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "spectron": "^3.6.4",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "sqlite3": "^3.1.8"
  }
}
